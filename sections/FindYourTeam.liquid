<!-- Find your team section starts -->
<section class="Find-your-team">
<div class="container">
<div class="welcome-outter">
{% if section.settings.title != blank %}
<h2 class="section-heading">{{ section.settings.title }}</h2>
{% endif %}
{%- assign item1 = section.settings.item_title_text_1 | handleize -%}   
{%- assign item2 = section.settings.item_title_text_2 | handleize -%}
{%- assign item3 = section.settings.item_title_text_3 | handleize -%} 
<div class="find-ur-team-main">
  <ul class="find-main-text-wrap">
    <li data-tab="{{item1}}" class="{{item1}}">{{ section.settings.item_title_text_1 }}<span> {% if section.settings.itemimage_1  != blank %}  <img class="find_item_image" src="{{ section.settings.itemimage_1  | img_url }}"> {% endif %}</span></li>     
    <li data-tab="{{item2}}" class="{{item2}}">{{ section.settings.item_title_text_2 }}<span>  {% if section.settings.itemimage_2  != blank %}  <img class="find_item_image" src="{{ section.settings.itemimage_2 | img_url }}"> {% endif %}</span></li>
    <li data-tab="{{item3}}" class="{{item3}}">{{ section.settings.item_title_text_3 }}<span>  {% if section.settings.itemimage_3  != blank %}   <img class="find_item_image" src="{{ section.settings.itemimage_3 | img_url }}"> {% endif %}</span></li>
  </ul>
</div>
 
<div class="find-ur-team-tab-wrap">  
{% if section.blocks.size > 0 %}            
{% for block in section.blocks %}
{%- assign block_link_handle = block.settings.listlink_title | handleize -%}
 
  {% if block_link_handle == item1 or block_link_handle == item2 or block_link_handle == item3 %} 
  <ul id="{{ block_link_handle }}" class="brand tab-content">
          {% for i in (1..50) %}
          {% capture image_string %}listimage_{{ forloop.index }}{% endcapture %}
          {%- assign image = block.settings[image_string] -%}
          {% capture url_string %}listimage_{{ forloop.index }}_url{% endcapture %}
          {%- assign url = block.settings[url_string] -%}
          {% capture btn_text_string %}listimage_{{ forloop.index }}_btn_text{% endcapture %}
          {%- assign btn_text = block.settings[btn_text_string] -%}

          {% if image != blank %}          
          <li class="find-team-listitem"><a href="{{ url }}" >
            <img class="find_image_listitem" src="{{ image | img_url }}" alt="{{ btn_text }}" >
            {% if btn_text != blank %}
            <span class="find_text_listitem">{{ btn_text }}</span>
            {% endif %}
            </a></li>          
          {% endif %}
          {% endfor %}
   </ul>
  {% endif %}
  
  
{% endfor %}
{% endif %}
</div>
</div>
</div>
</section>
<!-- Find your team section ends -->

